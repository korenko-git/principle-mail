{
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
  },
  "env": {
      "es6": true,
      "browser": true,
      "node": true
  },
  "extends": [
      "eslint:recommended",
      "google"
  ],
  "rules": {
    "require-jsdoc" : 0,
    "indent": ["error", 2],
    "max-len": 0
  }
}